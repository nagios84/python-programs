#This program calculates the future value of 10 year investment

print "This program calculates the future value of 10 year investment"
def futVal():
    principal = float(raw_input("Enter the intial principal : "))
    apr = float(raw_input("Enter the annualized intrest rate : "))
    for i in range(10):
        principal = principal*(1+apr)
        print "The amount in ten years will be" , principal
    

futVal()
